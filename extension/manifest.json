{
    "manifest_version": 2,
    "name": "Detect Genre",
    "version": "1.0",
    
    
    "content_scripts": [{
        "js": ["popup.js"],
        "matches": ["http://*/*", "https://*/*"]
      }],
    
    
    "permissions": [  "activeTab", "declarativeContent", "storage",       "tabs",         "http://*/*"     ],
  
        "browser_action": {
          "default_title": "Detect Genre",
          
          "default_popup": "popup.html"
      }
      
  }